{
  "name": "avo-inspector",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --emitDeclarationOnly && parcel build src/index.ts",
    "dev": "parcel src/index.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avohq/web-avo-inspector.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "examples"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "keywords": [
    "web",
    "analytics",
    "avo",
    "react"
  ],
  "author": "Avo (https://www.avo.app)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avohq/web-avo-inspector/issues"
  },
  "homepage": "https://github.com/avohq/web-avo-inspector#readme",
  "dependencies": {
    "universal-cookie": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@types/jest": "25.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "jest": "^25.4.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "ts-jest": "25.4.0",
    "typescript": "^3.8.3"
  }
}
